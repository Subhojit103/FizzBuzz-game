count_fizzbuzz=0
count_fizz=0
count_buzz=0

for number in range(1, 301):
  if number % 3 == 0 and number % 5 == 0:
    print("FizzBuzz")
    count_fizzbuzz +=1
  elif number%3==0:  
    print("Fizz")
    count_fizz+=1
  elif number % 5 == 0:
    print("Buzz")
    count_buzz+=1
  else:
    print(number)

print(f" no of fizzbuzz is : {count_fizzbuzz}")
print(f" no of fizz is : {count_fizz}")
print(f" no of buzz is : {count_buzz}")
  
